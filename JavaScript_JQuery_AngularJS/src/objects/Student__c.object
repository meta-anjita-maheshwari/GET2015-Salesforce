    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>DOBVelidation</fullName>
        <active>true</active>
        <errorConditionFormula>Date_Of_Birth__c  &gt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Date_Of_Birth__c</errorDisplayField>
        <errorMessage>Student DOB can not be greater than TODAY</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Delete_Students</fullName>
        <availability>online</availability>
        <description>Deleting selected students</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete Students</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
var url = parent.location.href;
var records = {!GETRECORDIDS($ObjectType.Student__c)};

if (records[0] == null) {
alert(&quot;Please select at least one record to update.&quot;);
} else {
window.location = &apos;apex/DeleteStudents?recs=&apos;+records.join(&apos;,&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>Generate_PDF</fullName>
        <availability>online</availability>
        <description>Generate PDF of student information</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Generate PDF</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}
sforce.connection.sessionId = &apos;{!$Api.Session_ID}&apos;; sforce.apex.execute(&quot;AttachPdf&quot;,&quot;savePdf&quot;,{}&quot;); 
window.location.reload();</url>
    </webLinks>
