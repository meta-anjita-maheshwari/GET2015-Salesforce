        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var myWindow = window.open(&quot;&quot;, &quot;MsgWindow&quot;, &quot;width=200, height=100&quot;); 
myWindow.document.write(&quot;&lt;table border=&apos;3&apos; cellspacing=&apos;4&apos;&gt;&quot;
+&quot;&lt;tr&gt;&quot;
