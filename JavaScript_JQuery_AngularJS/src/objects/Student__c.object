    <fields>
        <fullName>Age__c</fullName>
        <description>Age of student</description>
        <externalId>false</externalId>
        <formula>(TODAY() -  Date_Of_Birth__c)/365.25</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Fill age of student.</inlineHelpText>
        <label>Age</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Class__c</fullName>
        <description>Class of the student.</description>
        <externalId>false</externalId>
        <inlineHelpText>Select class of student.</inlineHelpText>
        <label>Class</label>
        <referenceTo>Class__c</referenceTo>
        <relationshipLabel>Students</relationshipLabel>
        <relationshipName>Students</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Date_Of_Birth__c</fullName>
        <description>Date of birth of student</description>
        <externalId>false</externalId>
        <inlineHelpText>Select date of birth of student</inlineHelpText>
        <label>Date Of Birth</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
