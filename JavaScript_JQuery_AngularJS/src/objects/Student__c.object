        <fullName>Sex__c</fullName>
        <externalId>false</externalId>
        <label>Sex</label>
        <picklist>
            <picklistValues>
                <fullName>Male</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Female</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
