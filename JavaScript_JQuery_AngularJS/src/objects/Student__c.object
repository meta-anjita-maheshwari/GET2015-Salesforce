    <label>Student</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>Show_Male_Students</fullName>
        <columns>FirstName__c</columns>
        <columns>Last_Name__c</columns>
        <columns>Date_Of_Birth__c</columns>
        <columns>Email__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Sex__c</field>
            <operation>equals</operation>
            <value>Male</value>
        </filters>
        <label>Show Male Students</label>
    </listViews>
