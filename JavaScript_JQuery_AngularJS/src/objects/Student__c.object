        <description>Last Name of student</description>
        <externalId>false</externalId>
        <inlineHelpText>Fill Last Name of student.</inlineHelpText>
        <label>Last Name</label>
        <length>20</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
